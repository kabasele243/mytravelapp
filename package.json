{
  "name": "mytravelapp",
  "version": "1.0.0",
  "description": "web application for users to reserve and book vacations tours.",
  "main": "app.js",
  "scripts": {
    "test": "jest --watch --runInBand",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:prod": "NODE_ENV=production nodemon server.js",
    "debug": "ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [
    "node.js",
    "experss",
    "mongodb"
  ],
  "author": "Franck Kabasele",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.2",
    "express-rate-limit": "^5.2.5",
    "helmet": "^3.23.3",
    "hpp": "^0.2.3",
    "html-to-text": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.15",
    "morgan": "^1.10.0",
    "multer": "^1.4.1",
    "ndb": "^1.1.5",
    "nodemailer": "^6.4.18",
    "pug": "^2.0.4",
    "sharp": "^0.27.1",
    "slugify": "^1.4.6",
    "stripe": "^8.135.0",
    "validator": "^13.5.2",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "env-cmd": "^8.0.2",
    "jest": "^26.6.3",
    "parcel-bundler": "^1.12.4",
    "supertest": "^3.4.1"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
